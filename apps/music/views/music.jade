doctype 5
html(lang="en")
	head		
		title music
		
		// Core inclusion
		link(rel='stylesheet', href='/core/css/style.css')
		script(src="/core/js/jquery-1.8.2.min.js")
		script(src="/core/js/jquery.mcjs.core.js")
		
		
		// App specific

		script
			$(document).ready(function(){
				$('body').mcjs();

			});
	body
		#backdrop
			img.backdropimg(src="/core/css/img/homepage.jpg")	
		#wrapper
			a.backlink(href="/") 
				img(src="/core/icons/back.png")
			h1 Music
			#musicWrapper
				if status !== null
					p.gui= status
				else
					ul.music
						each album in music
							li
								a.title= album		